{
    "name": "perederko/laravel-admin-elfinder-ext",
    "description": "elFinder extension for the laravel-admin",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/perederko/elfinder-flysystem-driver"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "illuminate/support": "5.0.x|5.1.x|5.2.x|5.3.x|5.4.x|5.5.x",
        "encore/laravel-admin": "1.5.*",
        "studio-42/elfinder": "2.1.*",
        "barryvdh/elfinder-flysystem-driver": "^0.2",
        "league/flysystem": "^1.0",
        "league/flysystem-cached-adapter": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Perederko\\Laravel\\Ext\\Admin\\ElFinder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Perederko\\Laravel\\Ext\\Admin\\ElFinder\\ElFinderServiceProvider"
            ]

        }
    }
}
